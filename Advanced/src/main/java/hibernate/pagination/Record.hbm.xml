<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.pagination">
    <class name="Record" table="tb_records">
        <id name="id" column="pk_record">
            <generator class="increment"/>
        </id>
        <property name="name"/>
    </class>
</hibernate-mapping>
