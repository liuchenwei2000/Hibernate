<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.lifecycle">
    <class name="Entity" table="tb_entity">
        <id name="id" column="pk_entity">
            <generator class="increment"/>
        </id>
        <property name="name"/>
    </class>
</hibernate-mapping>
