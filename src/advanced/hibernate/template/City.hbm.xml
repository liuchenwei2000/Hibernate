<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.template">
    <class name="City" table="tb_cities">
        <id name="id" column="pk_city">
            <generator class="native"/>
        </id>
        <property name="name"/>
    </class>
</hibernate-mapping>
